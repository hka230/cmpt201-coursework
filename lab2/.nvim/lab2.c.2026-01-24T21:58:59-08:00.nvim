#define _POSIX_C_SOURCE 200809L
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <unistd.h>
#include <sys/wait.h>

int main() {
  char *buff = NULL;
  size_t size = 0;

  while (getline(&buff, &size, stdin) != -1) {
    
    char *tok;
    char *saveptr;
    tok = strtok_r(buff, "\n", &saveptr);
    
    printf("%s\n",tok);
    //pid_t pid = fork();

  }
  perror("no getline input, ending program");
  free(buff);
}
